<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:DiagramDesigner;assembly=DiagramDesigner" 
                    xmlns:c="clr-namespace:DiagramDesigner.Controls;assembly=DiagramDesigner" 
                    xmlns:diagram="clr-namespace:Misp.Sourcing.AllocationDiagrams" >


    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="MispStyle.xaml"/>        
    </ResourceDictionary.MergedDictionaries>

    <s:Toolbox x:Key="DiagramToolbox" ItemSize="70,50">
        <ItemsControl.Items>
            <diagram:AllocationBoxItem></diagram:AllocationBoxItem>
        </ItemsControl.Items>
    </s:Toolbox>
</ResourceDictionary>