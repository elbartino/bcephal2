<Window x:Class="Misp.Sourcing.AutomaticSourcingViews.AutomaticSourcingNewValues"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Icon="/bcephal;Component/Resources/Images/Logo/bcephal.ico"
        Title="Update universe"  Width="530" WindowStartupLocation="CenterScreen">
        
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="40"/>
        </Grid.RowDefinitions>
         <ScrollViewer Grid.Row="0" x:Name="AttributeList" x:FieldModifier="public" />
        <Grid Grid.Row="1" >
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="20*" />
                <ColumnDefinition Width="10*"/>
                <ColumnDefinition Width="10*"/>
            </Grid.ColumnDefinitions>
            <Button  Grid.Column="1" Margin="10,5,10,5" Content="Cancel" Height="25" Click="OnCancelUpdateUniverse" />
            <Button Grid.Column="2" Margin="10,5,10,5" Content="OK" Height="25" Click="OnRequestUpdateUniverse"/>
        </Grid>
    </Grid>
</Window>
