<Grid
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:dxsps="http://schemas.devexpress.com/winfx/2008/xaml/spreadsheet" x:Class="Misp.Kernel.Ui.Office.DevExpres.SpreedSheet" 
            mc:Ignorable="d">
    <Grid.RowDefinitions>
        <RowDefinition Height="Auto"/>
        <RowDefinition Height="1*"/>
    </Grid.RowDefinitions>
    <dxsps:SpreadsheetFormulaBarControl x:Name="formulaBar" Grid.Row="0" />
    <dxsps:SpreadsheetControl x:Name="SpreadSheet" Grid.Row="1"/>
</Grid>
