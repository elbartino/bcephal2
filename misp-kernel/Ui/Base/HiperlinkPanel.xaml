<Grid x:Class="Misp.Kernel.Ui.Base.HiperlinkPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:Ui="clr-namespace:Misp.Kernel.Ui.Base"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid.RowDefinitions>
        <RowDefinition Height="50" />
        <RowDefinition Height="*" />
    </Grid.RowDefinitions>
    <Label x:Name="TitleLabel" x:FieldModifier="public" Content="Title" Margin="0,0,0,10" Grid.Row="0" FontWeight="Bold" FontSize="12"/>
    <StackPanel x:Name="ItemsPanel" x:FieldModifier="public" Grid.Row="1"/>
</Grid>
