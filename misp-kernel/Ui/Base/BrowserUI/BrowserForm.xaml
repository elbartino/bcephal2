<Grid x:Class="Misp.Kernel.Ui.Base.BrowserUI.BrowserForm"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
             xmlns:src="clr-namespace:Misp.Kernel.Ui.Base.BrowserUI"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="700">
    
    <Grid.RowDefinitions>
        <RowDefinition Height="*"/>
        <RowDefinition Height="Auto"/>
    </Grid.RowDefinitions>
    
    <src:BrowserGrid x:Name="Grid" x:FieldModifier="public" Grid.Row="0"/>
    <src:PaginationBar x:Name="PaginationBar" x:FieldModifier="public" Grid.Row="1" HorizontalAlignment="Center"/>
    
</Grid>
