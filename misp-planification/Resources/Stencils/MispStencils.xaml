<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:DiagramDesigner;assembly=DiagramDesigner" 
                    xmlns:c="clr-namespace:DiagramDesigner.Controls;assembly=DiagramDesigner" 
                    xmlns:diagram="clr-namespace:Misp.Planification.Diagram" >


    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="MispStyle.xaml"/>        
    </ResourceDictionary.MergedDictionaries>

    <s:Toolbox x:Key="DiagramToolbox" ItemSize="70,50">
        <ItemsControl.Items>
            <diagram:LoopItem></diagram:LoopItem>
            <diagram:ActionItem></diagram:ActionItem>            
        </ItemsControl.Items>
    </s:Toolbox>


</ResourceDictionary>